for i in range(4):
  for k in range(5):
    # i,k를 활용할 수 있을까?
    # 어떻하면 음수를 만들 수 있을까?
    if i % 2 == 1:
      print(f'{i*-1}',end="")
    else:
      print(f'{i}',end="")
  print()
print("================")
sw = -1
for i in range(1,6):
  for k in range(5):
      print(f'{i * sw}',end="")
  sw = sw*-1
  print()
print("================")
sw = 1
for i in range(1,6):
  for k in range(5):
      print(f'{i * sw}',end="")
  sw = sw*-1
  print()
